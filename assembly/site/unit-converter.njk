    <meta name="twitter:description" content="{{ unitConverter[locale].meta.twitterDescription }}">
    <meta name="twitter:image" content="https://serpmonn.ru/frontend/images/Banner_Serpmonn.png">
    
    <!-- –ö–∞–Ω–æ–Ω–∏—á–µ—Å–∫–∞—è —Å—Å—ã–ª–∫–∞ -->
    {% if locale == 'ru' %}
    <link rel="canonical" href="https://serpmonn.ru/frontend/tools/engineering/unit-converter.html" />
    {% else %}
    <link rel="canonical" href="https://serpmonn.ru/frontend/{{ locale }}/tools/engineering/unit-converter.html" />
